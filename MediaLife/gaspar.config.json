{
	"Models": {
		"Include": [
			"./**/*.cs",
			"../MediaLife.Library/**/*.cs"
		],
		"Output": [
			{
				"Type": "TypeScript",
				"Location": "./Scripts/Models/~csharpe-models.ts"
			},
			{
				"Type": "TypeScript",
				"Location": "../MediaServer/macos/cs-models.ts"
			}
		],
		"NumericEnums": true,
		"StringLiteralTypesInsteadOfEnums": false
	},
	"Controllers": {
		"Output": [
			{
				"Type": "TypeScript",
				"Location": "./Scripts/Services/~csharpe-services.ts",
				"ModelPath": "../Models/~csharpe-models",
				"ErrorHandlerPath": "./service-error-handler",
				"DefaultErrorMessage": "None"
			}
		],
		"ServiceName": "MediaLife"
	}
}